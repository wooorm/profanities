{
  "name": "profanities",
  "version": "2.9.0",
  "description": "List of (possible) profane words",
  "license": "MIT",
  "keywords": [
    "offensive",
    "profane",
    "vulgar",
    "word",
    "list"
  ],
  "repository": "words/profanities",
  "bugs": "https://github.com/words/profanities/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
    "Michael Jalkio <mjalkio@gmail.com>",
    "Ron Michael Zettlemoyer <ron@zettlemoyer.com>"
  ],
  "main": "index.json",
  "files": [
    "ar-latn.json",
    "es.json",
    "fr.json",
    "index.json",
    "pt-br.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "JSONStream": "^1.1.2",
    "browserify": "^16.0.0",
    "got": "^9.0.0",
    "join-stream": "0.0.0",
    "map-stream": "0.0.7",
    "merge-stream": "^1.0.0",
    "nlcst-normalize": "^2.0.0",
    "prettier": "^1.14.3",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "sort-stream": "^1.0.1",
    "split": "^1.0.0",
    "stream-filter": "^2.1.0",
    "tape": "^4.0.0",
    "tinyify": "^2.4.3",
    "unique-stream": "^2.2.0",
    "wrap-stream": "^2.0.0",
    "xo": "^0.24.0"
  },
  "scripts": {
    "generate": "node script/build-data.js",
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "build-support": "node script/build-support.js",
    "build-bundle": "browserify index.json -s profanities > profanities.js",
    "build-mangle": "browserify index.json -s profanities -p tinyify > profanities.min.js",
    "build": "npm run build-support && npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test": "npm run generate && npm run format && npm run build && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "ignores": [
      "profanities.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
