{
  "name": "profanities",
  "version": "2.4.0",
  "description": "List of (possible) English profane words",
  "license": "MIT",
  "keywords": [
    "offensive",
    "profane",
    "vulgar",
    "word",
    "list"
  ],
  "repository": "https://github.com/wooorm/profanities",
  "bugs": "https://github.com/wooorm/profanities/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.json"
  ],
  "main": "index.json",
  "dependencies": {},
  "devDependencies": {
    "JSONStream": "^1.1.2",
    "browserify": "^14.0.0",
    "esmangle": "^1.0.0",
    "got": "^6.1.0",
    "join-stream": "0.0.0",
    "map-stream": "0.0.7",
    "merge-stream": "^1.0.0",
    "nlcst-normalize": "^2.0.0",
    "remark-cli": "^3.0.0",
    "remark-preset-wooorm": "^2.0.0",
    "sort-stream": "^1.0.1",
    "split": "^1.0.0",
    "stream-filter": "^2.1.0",
    "tape": "^4.0.0",
    "unique-stream": "^2.2.0",
    "wrap-stream": "^2.0.0",
    "xo": "^0.18.0"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-data": "node script/build-data.js",
    "build-support": "node script/build-support.js",
    "build-bundle": "browserify index.json --bare -s profanities > profanities.js",
    "build-mangle": "esmangle profanities.js > profanities.min.js",
    "build": "npm run build-md && npm run build-data && npm run build-support && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test": "npm run build && npm run lint && npm run test-api"
  },
  "xo": {
    "space": true,
    "ignores": [
      "profanities.js"
    ]
  },
  "remarkConfig": {
    "plugins": ["preset-wooorm"]
  }
}
